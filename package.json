{
  "name": "crossauth",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./dist/server/*": "./dist/server/*",
    "./dist/client/*": "./dist/client/*",
    "./*": "./*"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "postinstall": "npx prisma generate && npx prisma db push --force-reset",
    "dev": "vite",
    "build": "npm run build:index && npm run build:client && npm run build:server && npm run build:indexiife && npm run build:clientiife",
    "build:noniife": "npm run build:index && npm run build:client npm run build:server",
    "build:index": "LIB_NAME=index vite build",
    "build:indexiife": "LIB_NAME=indexiife vite build --emptyOutDir=false",
    "build:client": "LIB_NAME=client vite build --emptyOutDir=false",
    "build:clientiife": "LIB_NAME=clientiife vite build --emptyOutDir=false",
    "build:server": "LIB_NAME=server vite build --emptyOutDir=false",
    "preview": "vite preview",
    "test": "LIB_NAME=index vitest",
    "doc": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "supertest": "^6.3.3",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fastify/cookie": "^9.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/view": "^8.2.0",
    "@prisma/client": "^5.7.1",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/nunjucks": "^3.2.6",
    "cookie-parser": "^1.4.6",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "fastify": "^4.25.2",
    "fastify-vite": "^3.0.0-beta.27",
    "nunjucks": "^3.2.4",
    "prisma": "^5.7.1",
    "sonic-boom": "^3.7.0"
  }
}
