{
	"name": "@crossauth/sveltekit",
	"version": "1.1.1",
	"license": "Apache-2.0",
	"description": "Sveltekit adapter for Crossauth authentication package",
	"private": false,
	"type": "module",
	"scripts": {
		"preparex": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "dotenv -e .env.unittest vitest --exclude src/demo.spec.ts",
		"testonce": "dotenv -e .env.unittest vitest run"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": ">=4.0.0"
	},
	"sideEffects": [
		"**/*.css"
	],
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.22",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^25.0.3",
		"@types/web": "^0.0.154",
		"autoprefixer": "^10.4.18",
		"daisyui": "^4.7.2",
		"dotenv": "^16.3.1",
		"dotenv-cli": "^7.3.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.0.0 <=5.3.3",
		"vite": "^5.0.3",
		"vite-plugin-dts": "^3.6.4",
		"vitest": "^1.6.1",
		"vitest-fetch-mock": "^0.2.2"
	},
	"dependencies": {
		"@crossauth/backend": "workspace:^",
		"@crossauth/common": "workspace:^",
		"@types/cookie": "^0.6.0",
		"@types/qrcode": "^1.5.5",
		"cookie": "^0.6.0",
		"formidable": "^3.5.1",
		"jwt-decode": "^4.0.0",
		"minimatch": "^10.0.1",
		"publint": "0.3.15",
		"qrcode": "^1.5.4"
	}
}
