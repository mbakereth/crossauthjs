  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "dotenv -e .env.unittest -- vitest --exclude **/excl_*",
    "testonce": "dotenv -e .env.unittest vitest run --exclude **/excl_*",
    "testexclonce": "dotenv -e .env.unittest vitest run excl_",
    "testall": "dotenv -e .env.unittest vitest run ",
    "testldap": "dotenv -e .env.private vitest run ldapstor ldapauth",
    "doc": "typedoc",
    "prisma": "dotenv -e .env.unittest pnpm exec prisma"
  },
